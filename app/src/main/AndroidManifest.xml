<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tsj.dat153">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="activities.NameListActivity"
            android:label="@string/activity_choose_name"/>
        <activity android:name="activities.ImageGalleryActivity"
            android:label="@string/activity_choose_picture"/>
        <activity android:name="activities.LearningModeActivity"
            android:label="@string/activity_learning"/>
        <activity android:name="activities.ShowPictureActivity" />
        <activity android:name=".activities.ShowNameActivity">
        </activity>
    </application>

</manifest>